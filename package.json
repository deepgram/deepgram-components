{
  "name": "deepgram-components",
  "version": "0.0.1",
  "description": "Deepgram Components with Stencil and Storybook",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/deepgram-components/deepgram-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./deepgram-header": {
      "import": "./dist/components/deepgram-header.js",
      "types": "./dist/components/deepgram-header.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepgram/deepgram-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "start:stencil": "stencil build --watch",
    "start:storybook": "storybook dev -p 6006",
    "start": "concurrently \"npm run start:stencil\" \"sleep 5 && npm run start:storybook\""
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@stencil/core": "^4.7.0",
    "@storybook/addon-essentials": "^8.6.0",
    "@storybook/addon-viewport": "^8.6.0",
    "@storybook/blocks": "^8.6.0",
    "@storybook/experimental-addon-test": "^8.6.0",
    "@storybook/test": "^8.6.0",
    "@storybook/web-components": "^8.6.0",
    "@storybook/web-components-vite": "^8.6.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.2.1",
    "puppeteer": "^24.3.0",
    "storybook": "^8.6.0",
    "vite": "^6.2.0"
  },
  "license": "MIT"
}
